{
  "host": "localhost",
  "serviceName":"automatic-service",
  "port": 3030,
  "public": "../public/",
  "paginate": {
    "default": 50,
    "max": 100
  },
  "log2Console":true,
  "logLevel":"debug",
  "logPath":"/media/data/",
  "net_interface":"eth0",
  "private_network": {
    "addr": "10.200.0.0",
    "whiteList":[]
  },
  "traefik_gateway": {
    "addr": "http://traefik:3028"
  },
  "emqtt_server": {
    "addr": "mqtt://emqtt",
    "username": "automatic-service",
    "password": "",
    "shareGroup":"automatic-service"
  },
  "mongodb": {
    "addr": "mongodb://mongodb:27017/test"
  },
  "redis": {
    "url": "redis://redis:6379"
  },
  "redisCache" : {
    "url" : "redis://redis-cache:6379",
     "defaultDuration": 172800,
     "parseNestedRoutes": false,
     "removePathFromCacheKey": false
   },
   "limit" : {
     "rule":32
   },
   "slack":{
    "token":"",
    "conversationId":""
  },
  "authentication": {
    "secret": "",
    "strategies": [
      "jwt"
    ],
    "service": "unuse",
    "jwt": {
      "audience": "",
      "subject": "",
      "issuer": "",
      "algorithm": "HS256",
      "expiresIn": "30d",
      "serviceByType":{
        "user":{
          "entity": "user",
          "service":"users"
        },
        "device":{
          "entity": "device",
          "service":"devices"
        }
      }
    }
  }
}
